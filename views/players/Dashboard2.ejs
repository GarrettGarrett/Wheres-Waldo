                                  <th>Position</th>
                                  <th>Time</th>
                                  <th>User</th>
                              </tr>
                          </thead>
                  
                          <tbody class='hiscore'>
                          <% for(let i = 0; i < players.length; i++) { %>
                          <% if (players[i].seascore > 0) { %>
                              <tr>
                              <td> <%= i + 1 %> </td>
                              <td><%= players[i].seascore %></td>
                              <td><%= players[i].username %></td>
                              </tr>
                          <% } %> 
